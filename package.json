{
    "name": "tsdocparser",
    "version": "0.1.0",
    "repository": "https://github.com/digojs/tsdocparser",
    "description": "Documentation comment parser for TypeScript & JavaScript",
    "author": "xuld <xuld@vip.qq.com>",
    "keywords": [
        "tsdoc",
        "jsdoc"
    ],
    "license": "MIT",
    "dependencies": {
        "typescript": "next"
    },
    "typings": "index.d.ts",
    "scripts": {
        "build": "tsc index.ts -d --target es6 --module commonjs",
        "watch": "tsc -w index.ts -d --target es6 --module commonjs",
        "test": "node -p \"JSON.stringify(require('./').default('index.ts'), null, 2)\"",
        "debug": "node --inspect-brk -p \"JSON.stringify(require('./').default('index.ts'), null, 2)\""
    },
    "files": [
        "index.js",
        "index.d.ts"
    ]
}